{
  "name": "x-flow-js-highlighter",
  "displayName": "X-Flow JS Highlighter",
  "version": "0.0.2",
  "publisher": "local",
  "engines": {
    "vscode": "^1.50.0"
  },
  "contributes": {
    "grammars": [
      {
        "path": "./x-flow-js-xml.tmLanguage.json",
        "scopeName": "injection.x-flow-js-xml",
        "injectTo": [
          "text.xml"
        ]
      },
      {
        "path": "./x-flow-js-html.tmLanguage.json",
        "scopeName": "injection.x-flow-js-html",
        "injectTo": [
          "text.html.basic",
          "text.html"
        ]
      }
    ]
  }
}
